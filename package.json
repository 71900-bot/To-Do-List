{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "styles": "postcss src/styles/global.pcss -o public/global.css",
    "styles:prod": "postcss src/styles/global.pcss --map --env production -o public/global.css",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@storybook/addon-actions": "^5.3.14",
    "@storybook/addon-links": "^5.3.14",
    "@storybook/addons": "^5.3.14",
    "@storybook/svelte": "^5.3.14",
    "@tailwindcss/custom-forms": "^0.2.1",
    "babel-loader": "^8.0.6",
    "cssnano": "^4.1.10",
    "postcss": "^7.0.27",
    "postcss-cli": "^7.1.0",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "rollup": "^1.20.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-postcss": "^2.3.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "svelte": "^3.19.2",
    "svelte-feather-icons": "^3.2.2",
    "svelte-loader": "^2.13.6",
    "tailwindcss": "^1.2.0"
  },
  "dependencies": {
    "sirv-cli": "^0.4.4"
  }
}
