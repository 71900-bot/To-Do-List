{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "test": "jest src",
    "test:watch": "jest --watch src",
    "styles": "postcss src/styles/global.pcss -o public/global.css",
    "styles:prod": "postcss src/styles/global.pcss --map --env production -o public/global.css",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.svelte$": "svelte-jester"
    },
    "moduleFileExtensions": [
      "js",
      "svelte"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@storybook/addon-actions": "^5.3.14",
    "@storybook/addon-centered": "^5.3.14",
    "@storybook/addon-knobs": "^5.3.14",
    "@storybook/addon-links": "^5.3.14",
    "@storybook/addons": "^5.3.14",
    "@storybook/svelte": "^5.3.14",
    "@tailwindcss/custom-forms": "^0.2.1",
    "@testing-library/jest-dom": "^5.1.1",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "cssnano": "^4.1.10",
    "jest": "^25.1.0",
    "postcss": "^7.0.27",
    "postcss-cli": "^7.1.0",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "rollup": "^1.20.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-postcss": "^2.3.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "svelte": "^3.19.2",
    "svelte-feather-icons": "^3.2.2",
    "svelte-jester": "^1.0.5",
    "svelte-loader": "^2.13.6",
    "tailwindcss": "^1.2.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "dayjs": "^1.8.22",
    "invariant": "^2.2.4",
    "sirv-cli": "^0.4.4"
  }
}
